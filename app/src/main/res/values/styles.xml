        <item name="android:background">@null</item>
        <item name="background">@null</item>
        <item name="android:titleTextStyle">@style/UIActionBar.TitleText</item>
        <item name="titleTextStyle">@style/UIActionBar.TitleText</item>
    </style>
    <style name="UIActionBar.TitleText" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/black</item>
