        <item name="android:textColorSecondary">@color/gray97</item>
    </style>
    <style name="LineColor">
        <item name="android:background">@color/gray66</item>
    </style>
    <style name="SliderColor" parent="Widget.MaterialComponents.Slider">
        <item name="colorPrimary">@color/gray117</item>
        <item name="colorOnPrimary">@color/gray224</item>
